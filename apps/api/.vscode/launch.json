{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Vitest Debug (current file)",
      "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
      "args": ["run", "${file}", "--inspect-brk"],
      "autoAttachChildProcesses": true,
      "skipFiles": ["<node_internals>/**"],
      "smartStep": true
    }
  ]
}
