{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"dev": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"test": {
			"dependsOn": ["^test"],
			"outputs": []
		},
		"clean": {
			"cache": false
		},
		"lint": {
			"outputs": []
		},
		"check-types": {
			"outputs": []
		}
	}
}
